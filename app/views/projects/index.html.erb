
<div class="container">
  	<div class="col-md-6 col-md-offset-3">
    	<div class="panel panel-default">
      		<div class="panel-body">

				<h1>Projects</h1>
				<%= link_to "New Project", new_project_path %>
				<div id="projects">
					<% @projects.each do |project| %>
						<h2>
							<%= link_to project.name, project %>
						</h2>
						<p>
							<%= project.description %>
						</p>
					<% end %>
				</div>

			</div>
		</div>
	</div>
</div>